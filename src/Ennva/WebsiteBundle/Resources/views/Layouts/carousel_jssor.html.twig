<div id="slider1_container" class="container-fluid"
	style="position: relative; top: 0px; left: 0px; height: 500px; width: 1260px; margin-bottom: 60px;">
	<!-- Slides Container -->
	<div u="slides"
		style="cursor: move; position: absolute; overflow: hidden; left: 0px; top: 0px; margin: 0 auto; width: 1260px; height: 500px;">
		<div class="first_slide">
			<img u="image"
				src="{{ asset('bundles/ennvawebsite/images/slider_background.jpg') }}" />
			<img u="thumb"
				src="{{ asset('bundles/ennvawebsite/images/thumbnails/thumb-crm2.jpg') }}" />
			<div u="caption" t="L" t2="T"
				style="position: absolute; top: 100px; left: 200px; width: 800px; height: 50px;">
				<p class="slide-image" style="z-index: 50;">
					<img alt="" style="border: solid 0px; width: 200px; height: 250px;"
						src="{{ asset('bundles/ennvawebsite/images/services/img_big_ati_icon.png') }}" 
						/>
				</p>
				<p class="img-rounded slide-title">
					<u style="font-family: 'Gloria Hallelujah',cursive;">Title</u>
				</p>
				<!-- You can move theses paragraph inside specific caption with specific transition -->
				<p class="img-rounded slide-footer">
				     <u>Footer</u><br>
				     <a class="btn btn-info " type="button" href="#">More Infos</a>
				</p>
			</div>
			<div u="caption" t="R" t2="B"
				style="position: absolute; top: 360px; left: 200px; width: 800px; height: 50px;">
				<p class="img-rounded slide-description">
				     <u>Description</u><br>
					Vivamus sagittis lacus vel augue laoreet rutrum faucibus dolor
					auctor. Duis mollis, est non commodo luctus.
					Vivamus sagittis lacus vel augue laoreet rutrum faucibus dolor
				</p>
			</div>
			<div u="caption" t="MCLIP|B" t2="NO"
				style="position: absolute; top: 450px; left: 0px; width: 1260px; height: 50px;">
				<div
					style="position: absolute; top: 0px; left: 0px; width: 1260px; height: 50px; background-color: Black; opacity: 0.5; filter: alpha(opacity = 50);">
				</div>
				<div
					style="position: absolute; top: 0px; left: 0px; width: 600px; height: 50px; color: White; font-size: 16px; font-weight: bold; line-height: 50px; text-align: center;">
					More Description of slider here: bla bla bla bla bla bla bla bla
				</div>
			</div>
			<div u="caption" t="WAVE|L" class=bricon
				style="position: absolute; top: 360px; left: 1050px; width: 30px; height: 30px; background-position: -120px 0px;"></div>
			<div u="caption" t="WAVE|L" d="-1350" class=bricon
				style="position: absolute; top: 360px; left: 1000px; width: 30px; height: 30px; background-position: -90px 0px;"></div>
			<div u="caption" t="WAVE|L" d="-1350" class=bricon
				style="position: absolute; top: 360px; left: 950px; width: 30px; height: 30px; background-position: -60px 0px;"></div>
			<div u="caption" t="WAVE|L" d="-1350" class=bricon
				style="position: absolute; top: 360px; left: 900px; width: 30px; height: 30px; background-position: -30px 0px;"></div>
			<div u="caption" t="WAVE|L" d="-1350" class=bricon
				style="position: absolute; top: 360px; left: 850px; width: 30px; height: 30px; background-position: 0px 0px;"></div>

		</div>
		<div class="second_slide">
			<img u="image"
				src="{{ asset('bundles/ennvawebsite/images/slider_background.jpg') }}" />
			<img u="thumb"
				src="{{ asset('bundles/ennvawebsite/images/crm.png') }}" />
			<div u="caption" t="caption-transition-name"
				style="position: absolute; top: 200px; left: 125px; width: 350px; height: 50px;">
				<h1>Second Text Here</h1>
			</div>
		</div>
		<div class="third_slide">
			<img u="image"
				src="{{ asset('bundles/ennvawebsite/images/slider_background.jpg') }}" />
			<div u="caption" t="caption-transition-name"
				style="position: absolute; top: 200px; left: 125px; width: 350px; height: 50px;">
				<h1>Third Text Here</h1>
			</div>
		</div>
		<!-- Navigator Skin Begin -->
		<div u="navigator" class="jssorb02"
			style="position: absolute; bottom: 70px; left: 6px;">
			<!-- bullet navigator item prototype -->
			<div u="prototype"
				style="POSITION: absolute; WIDTH: 21px; HEIGHT: 21px; text-align: center; line-height: 21px; color: White; font-size: 12px;">
				<div u="numbertemplate"></div>
			</div>
		</div>
		<!-- Navigator Skin End -->

		<!-- Arrow Navigator Skin Begin -->
		<!-- Arrow Left -->
		<span u="arrowleft" class="jssora01l"
			style="width: 45px; height: 45px; top: 123px; left: 8px;"> </span>
		<!-- Arrow Right -->
		<span u="arrowright" class="jssora01r"
			style="width: 45px; height: 45px; top: 123px; right: 8px"> </span>
